{
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaVersion": 8,
    "sourceType": "module"
  },
  "env": {
    "node": true,
    "jest": true,
    "es6": true
  },
  "rules": {
    "no-console": 0,
    "no-unused-vars": 1,
    "import/prefer-default-export": 0,
    "no-iterators/generators": 0,
    "no-restricted-syntax": 0,
    "no-await-in-loop": 0,
    "no-underscore-dangle": 0
  },
  "plugins": ["jest"]
}
